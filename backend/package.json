{
  "name": "processo-seletivo-davin-ti",
  "version": "1.0.0",
  "description": "projetinho do processo seletivo da davin ti",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "build": "rimraf dist && tsup src/index.ts --format cjs,esm --dts --minify",
    "migrate": "drizzle-kit generate --out ./drizzle --schema ./src/db/schema.ts",
    "db:push": "drizzle-kit push --schema ./src/db/schema.ts",
    "db:pull": "drizzle-kit pull --schema ./src/db/schema.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "keywords": [],
  "author": "Davi Dias",
  "license": "UNLICENSED",
  "type": "commonjs",
  "dependencies": {
    "dotenv": "^17.3.1",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "pg": "^8.18.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.0",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.9",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
